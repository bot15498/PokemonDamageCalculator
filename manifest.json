{
    "name" : "PokemonDamageCalculator",
    "version" : "1.0",
    "description" : "A damage calculator helper for Pokemon players",
    "manifest_version" : 2,
    "minimum_chrome_version" : "10.0",
    "devtools_page" : "devtools.html",
    "icons": {
        "128": "icon.png"
      },
    "browser_action": {
      "default_title": "Damage Calculator for Pokemon Showdown!",
      "default_popup": "popup.html"
    },
    "permissions" : [
      "identity"
    ],
    "content_security_policy" : "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
  }